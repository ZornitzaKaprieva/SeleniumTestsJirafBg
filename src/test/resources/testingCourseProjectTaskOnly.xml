<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite thread-count="3" verbose="1" name="SeleniumTestsJirafBg"
       annotations="JDK" parallel="tests">


    <test name="LoginSuit" >
        <classes>
            <class name="testsPOM.logIn.LogInTestCorrectCredentials" />
            <class name="testsPOM.logIn.LogInTestWrongCredentials" />
        </classes>
    </test>


    <test name="AddToCartSuit">
        <classes>
            <class name="testsPOM.addToCart.AddToCart3ItemsFromHomePageAfterLogIn"/>
        </classes>
    </test>

    <test name="CheckoutSuit">
        <classes>
            <class name="testsPOM.checkout.GoToCheckOutWithOneProductFromEachCategoryWithoutApprovalWindow"/>
        </classes>
    </test>

</suite>
