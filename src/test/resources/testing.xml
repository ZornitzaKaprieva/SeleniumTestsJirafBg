<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite thread-count="3" verbose="1" name="SeleniumTestsJirafBg"
       annotations="JDK" parallel="tests">

    <test name="HomePageSuit" >
        <classes>
            <class name="testsPOM.homePage.HomePageTest" />
        </classes>
    </test>

    <test name="LoginSuit" >
        <classes>
            <class name="testsPOM.logIn.LogInTestCorrectCredentials" />
            <class name="testsPOM.logIn.LogInTestWrongCredentials" />
        </classes>
    </test>

    <test name="MyProfileSuit">
        <classes>
            <class name="testsPOM.myProfile.MyProfilePageGoToHomePage"/>
            <class name="testsPOM.myProfile.MyProfilePageTestLogOut"/>
        </classes>
    </test>

    <test name="SelectItemsSuit">
        <classes>
            <class name="testsPOM.selectItemAfterLogIn.SelectItemFromHomePageAfterLogIn"/>
        </classes>
    </test>

    <test name="AddToCartSuit">
        <classes>
            <class name="testsPOM.addToCart.AddToCartItemFromHomePage"/>
            <class name="testsPOM.addToCart.AddToCart3ItemsFromHomePage"/>
        </classes>
    </test>

</suite>
